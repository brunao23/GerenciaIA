{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean:all": "rimraf .next node_modules package-lock.json && npm install --legacy-peer-deps",
    "clean:install": "npm run kill:all && npm install --legacy-peer-deps",
    "clear:cache": "npm run kill:all && rimraf .next node_modules/.cache && npm cache clean --force",
    "dev": "next dev",
    "dev:3001": "next dev -p 3001",
    "dev:3002": "next dev -p 3002",
    "dev:8080": "next dev -p 8080",
    "fix:build": "rimraf .next node_modules && npm install --legacy-peer-deps && npm run build",
    "fix:paths": "rimraf .next && npm cache clean --force && npm install --legacy-peer-deps",
    "kill:all": "node -e \"process.platform === 'win32' ? require('child_process').exec('taskkill /f /im node.exe & taskkill /f /im npm.exe') : require('child_process').exec('pkill -f node; pkill -f npm')\"",
    "lint": "next lint",
    "restart:dev": "npm run clear:cache && npm run dev",
    "setup": "npm run kill:all && npm install --legacy-peer-deps && npm run dev",
    "start": "next start"
  },
